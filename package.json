{
  "name": "sanity-plugin-automatic-tagger",
  "version": "1.0.0",
  "description": "A Sanity Studio v3 plugin that automatically generates SEO keywords and tags for article content using AI-powered analysis via OpenRouter API",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "seo",
    "keywords",
    "ai",
    "openrouter",
    "tags",
    "automatic",
    "content-analysis"
  ],
  "homepage": "https://github.com/Jsnnmsc/sanity-plugin-automatic-tagger#readme",
  "bugs": {
    "url": "https://github.com/Jsnnmsc/sanity-plugin-automatic-tagger/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Jsnnmsc/sanity-plugin-automatic-tagger.git"
  },
  "license": "MIT",
  "author": "Jsnn <jsnnmsc0421@gmail.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/studio-secrets": "^3.0.2"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^8.1.17",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^19.1.15",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanity": "^4.10.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^3 || ^4"
  },
  "engines": {
    "node": ">=18"
  }
}
